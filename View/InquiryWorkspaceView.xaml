<UserControl x:Class="AdmissionsInformationSystem.View.InquiryWorkspaceView"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Width="640" Height="255">
	<Grid Height="255" Margin="0,0,-155.447,0" Width="640" HorizontalAlignment="Left">
		<Grid.RowDefinitions>
			<RowDefinition Height="14*"/>
			<RowDefinition Height="51*"/>
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="93*"/>
			<ColumnDefinition Width="191*"/>
			<ColumnDefinition Width="180*"/>
			<ColumnDefinition Width="176*"/>
		</Grid.ColumnDefinitions>
		<TextBox HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="171" Grid.Column="1" Grid.Row="1"/>
		<TextBox HorizontalAlignment="Left" Height="23" Margin="10,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="171" Grid.Column="1" Grid.Row="1"/>
		<TextBox HorizontalAlignment="Left" Height="23" Margin="10,66,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="171" Grid.Column="1" Grid.Row="1"/>
		<TextBlock HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="First Name" VerticalAlignment="Top" Width="80" Height="16" Grid.Row="1"/>
		<TextBlock HorizontalAlignment="Left" Margin="10,38,0,0" TextWrapping="Wrap" Text="Last Name" VerticalAlignment="Top" Width="80" Height="16" Grid.Row="1"/>
		<TextBlock HorizontalAlignment="Left" Margin="10,66,0,0" TextWrapping="Wrap" Text="Middle Initial" VerticalAlignment="Top" Width="80" Height="16" Grid.Row="1"/>
		<TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="&#x9;Thank you for your interest in studying at UNCC. To receive information about academic programs, fill out the form below and click the 'Inquire' button. To pre-apply for admission click the 'Apply' button and your information will be sent to the admissions office for review." VerticalAlignment="Top" Grid.ColumnSpan="4" Width="620" Margin="10,5,0,0" Height="50"/>
		<TextBox HorizontalAlignment="Left" Height="40" Margin="10,150,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="171" Grid.Column="1" Grid.Row="1"/>
		<TextBox HorizontalAlignment="Left" Height="23" Margin="10,122,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="171" Grid.Column="1" Grid.Row="1"/>
		<TextBox HorizontalAlignment="Left" Height="23" Margin="10,94,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="171" Grid.Column="1" Grid.Row="1"/>
		<TextBlock HorizontalAlignment="Left" Margin="10,150,0,0" TextWrapping="Wrap" Text="Address" VerticalAlignment="Top" Width="80" Height="16" Grid.Row="1"/>
		<TextBlock HorizontalAlignment="Left" Margin="10,122,0,0" TextWrapping="Wrap" Text="Phone Number" VerticalAlignment="Top" Width="93" Height="16" Grid.Row="1" Grid.ColumnSpan="2"/>
		<TextBlock HorizontalAlignment="Left" Margin="10,94,0,0" TextWrapping="Wrap" Text="E-mail" VerticalAlignment="Top" Width="80" Height="16" Grid.Row="1"/>
		<TextBox HorizontalAlignment="Left" Height="23" Margin="130,167,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="40" Grid.Column="2" Grid.Row="1"/>
		<TextBox HorizontalAlignment="Left" Height="23" Margin="45,167,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="40" Grid.Column="2" Grid.Row="1"/>
		<TextBlock HorizontalAlignment="Left" Margin="95,167,0,0" TextWrapping="Wrap" Text="SAT" VerticalAlignment="Top" Width="30" Height="16" Grid.Row="1" Grid.Column="2"/>
		<TextBlock HorizontalAlignment="Left" Margin="10,167,0,0" TextWrapping="Wrap" Text="GPA" VerticalAlignment="Top" Width="30" Height="16" Grid.Row="1" Grid.Column="2"/>
		<TextBlock HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Degree Programs" VerticalAlignment="Top" Width="100" Height="16" Grid.Row="1" Grid.Column="2"/>
		<ComboBox Grid.Column="2" HorizontalAlignment="Left" Margin="115,10,0,0" Grid.Row="1" VerticalAlignment="Top" Width="231" Grid.ColumnSpan="2"/>
		<TextBlock HorizontalAlignment="Left" Margin="10,85,0,0" TextWrapping="Wrap" Text="Personal Interests" VerticalAlignment="Top" Width="100" Height="16" Grid.Row="1" Grid.Column="2"/>
		<ComboBox Grid.Column="2" HorizontalAlignment="Left" Margin="115,85,0,0" Grid.Row="1" VerticalAlignment="Top" Width="231" Grid.ColumnSpan="2"/>
		<ListBox Grid.Column="2" HorizontalAlignment="Left" Height="41" Margin="10,35,0,0" Grid.Row="1" VerticalAlignment="Top" Width="336" Grid.ColumnSpan="2"/>
		<ListBox Grid.Column="2" HorizontalAlignment="Left" Height="41" Margin="10,110,0,0" Grid.Row="1" VerticalAlignment="Top" Width="336" Grid.ColumnSpan="2"/>
		<Button Content="Inquire" Grid.Column="3" HorizontalAlignment="Left" Margin="10,170,0,0" Grid.Row="1" VerticalAlignment="Top" Width="75" Height="20" Command="{Binding Path=InquireCommand}"/>
		<Button Content="Apply" Grid.Column="3" HorizontalAlignment="Left" Margin="91,170,0,0" Grid.Row="1" VerticalAlignment="Top" Width="75" Height="20" Command="{Binding Path=ApplyCommand}"/>
	</Grid>
</UserControl>